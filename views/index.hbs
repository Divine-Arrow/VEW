    <!--about and contact us-->
       
        <section id="contact">
            <div class="container custom-width ">
                <div class="row custom-about">
                    <div id="about_us" class="col-lg-6 col-sm-12 text-center custom-effect3">
                        <div class="product-header">
                            <p>
                                <i class="fas fa-circle"></i>
                                <i class="fas fa-circle"></i>
                                <span class="left">About</span>
                                <span class="right"> us</span>
                                <i class="fas fa-circle"></i>
                                <i class="fas fa-circle"></i>
                            </p>
                        </div>

                        <div class="custom-about-contact-col">
                            <img src="../public/home/images/about-me-image.png" alt="image">
                            <p class="text-center custom-about-p">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Temporibus, maxime amet enim eligendi dolore illum distinctio excepturi magnam cum nesciunt rerum, adipisci eos impedit porro culpa minus? Quisquam, necessitatibus, impedit.</p>
                        </div>
                    </div>
                    <div id="contact_us" class="col-lg-6 col-sm-12 text-center custom-effect4">
                        <div class="product-header">
                            <p>
                                <i class="fas fa-circle"></i>
                                <i class="fas fa-circle"></i>
                                <span class="left">Contact</span>
                                <span class="right">us</span>
                                <i class="fas fa-circle"></i>
                                <i class="fas fa-circle"></i>
                            </p>
                            {{msg}}
                        </div>
                        <div class="custom-about-contact-col custom-height custom-margin">
                            <form method="post" action="/">
                                <div class="form-row">
                                    <div class="form-group col-sm-6 position-relative">
                                        <i class="fas fa-user position-absolute custom-pos1"></i>
                                        <input type="text" class="form-control custom-form1" name="userName" id="inputName" placeholder="Your Name" required="">
                                    </div>
                                    <div class="form-group col-sm-6 position-relative">
                                        <i class="fas fa-envelope position-absolute custom-pos2"></i>
                                        <input type="email" class="form-control custom-form2" name="userid" id="inputEmail4" placeholder="Email" required="">
                                    </div>

                                    <div class="form-group col-sm-6 position-relative">
                                        <i class="fas fa-mobile-alt position-absolute custom-pos1"></i>
                                        <input type="text" class="form-control custom-form1" name="userNumber" id="inputPhone" placeholder="Phone No." required="">
                                    </div>
                                    <div class="form-group col-sm-6 position-relative">
                                        <i class="fas fa-address-card position-absolute custom-pos2"></i>
                                        <input type="text" class="form-control custom-form2" id="inputAddress" placeholder="Address" name="userAdress" required="">
                                    </div>
                                    <div class="form-group col-sm-12">
                                        <textarea class="form-control custom-textarea" name="userMessage" placeholder="Your Message" rows="3"></textarea>
                                    </div>
                                </div>
                                <div class="col-auto">
                                    <button type="submit" class="btn mb-2">Send Now</button>
                                </div>
                            </form>
                        </div>

                    </div>
                </div>
            </div>


        </section>


